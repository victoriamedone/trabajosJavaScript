const prompt = require('prompt-sync')();

// Tienes una lista de nombres ['Carlos', 'Daniel', 'Laura', 'Ana']. Queremos
// saber si el nombre 'Laura' está presente en la lista y, si lo está, deberías
// devolverlo.

let nombres = ['Carlos', 'Daniel', 'Laura', 'Ana'];

console.log("La lista ingresada es:",nombres);

function encontrarLaura(nombres){
    return nombres === "Laura"
};

let buscada = nombres.find(nombre => encontrarLaura(nombre))

console.log("El nombre buscado (dentro de la lista) es:",buscada);

